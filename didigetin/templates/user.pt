<!doctype html><html>  <head>    <meta charset="utf-8">    <title>Did ${userdata.name or 'I'} get in?</title>    <link rel="stylesheet" type="text/css" href="${request.route_url('css')}"/>  </head>  <body>    <h2 tal:condition="userdata.name">Did ${userdata.name} get in?</h2>    <h2 tal:condition="not:userdata.name">No data on the user ${request.matchdict['user']}</h2>    <table tal:condition="userdata.name"><tbody>    <tal:block tal:repeat="result resultlist">    <tr tal:condition="result.result"><td class="college"><a href="${request.route_url('college', user=userdata.name, college=result.url)}">${result.name}</a></td><td class="result">${result.result}</td></tr>    </tal:block>    </tbody></table>  </body></html><!-- change secret -->